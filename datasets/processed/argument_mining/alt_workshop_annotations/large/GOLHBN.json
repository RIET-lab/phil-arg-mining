        }
      ],
      "members": [
        {
          "type": "statement",
          "role": "relation-statement",
          "text": "Politics constitutes an inauthentic mode of being.",
          "startLine": 8,
          "startColumn": 6,
          "endLine": 8,
          "endColumn": 56,
          "title": "Untitled 8"
        }
      ],
      "isUsedAsRelationStatement": true,

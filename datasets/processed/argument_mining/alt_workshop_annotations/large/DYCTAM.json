      "members": [
        {
          "type": "statement",
          "role": "top-level-statement",
          "isTopLevel": true,
          "text": "The text’s main thesis: Only a plural, open conception of Enlightenment (as in Mendelssohn) does justice to women’s actions, agency, and philosophical thought; in this view, culture, education, and enlightenment are universal and emancipatory paths for all humanity.",
          "title": "Untitled 1",
          "startLine": 1,
          "startColumn": 1,
          "endLine": 16,
          "endColumn": 26
        }
      ],
      "isUsedAsTopLevelStatement": true,
      "fontColor": "#000000",
      "color": "#1b9e77"
    },
    "Untitled 2": {
      "type": "equivalence-class",
      "title": "Untitled 2",
      "relations": [
        {

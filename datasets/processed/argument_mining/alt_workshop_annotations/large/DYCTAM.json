          "fromType": "equivalence-class",
          "to": "Untitled 2",
          "toType": "equivalence-class"
        },
        {
          "type": "relation",
          "relationType": "support",
          "from": "Untitled 5",
          "fromType": "equivalence-class",
          "to": "Untitled 2",
          "toType": "equivalence-class"
        },
        {
          "type": "relation",
          "relationType": "support",
          "from": "Untitled 12",
          "fromType": "equivalence-class",
          "to": "Untitled 2",

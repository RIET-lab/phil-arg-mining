      "isUsedAsRelationStatement": true,
      "fontColor": "#000000",
      "color": "#1b9e77"
    },
    "Untitled 13": {
      "type": "equivalence-class",
      "title": "Untitled 13",
      "relations": [],
      "members": [
        {
          "type": "statement",
          "role": "top-level-statement",
          "isTopLevel": true,
          "text": "Currie’s theory is most plausible among the three, recognizing motion in cinema as real (though response-dependent), without fully embracing illusionism.",
          "startLine": 16,
          "startColumn": 1,
          "endLine": 16,
          "endColumn": 153,
          "title": "Untitled 13"
        }
      ],
      "isUsedAsTopLevelStatement": true,

          "type": "relation",
          "relationType": "support",
          "from": "Untitled 12",
          "fromType": "equivalence-class",
          "to": "Untitled 9",
          "toType": "equivalence-class"
        }
      ],
      "members": [
        {
          "type": "statement",
          "role": "top-level-statement",
          "isTopLevel": true,
          "text": "It is imperative that we attend to the social value of creativity. ",
          "title": "Untitled 9",
          "startLine": 10,
          "startColumn": 1,
          "endLine": 14,
          "endColumn": 24
        }
      ],
      "isUsedAsTopLevelStatement": true,
      "fontColor": "#000000",
      "color": "#1b9e77"
    },
    "Untitled 10": {
      "type": "equivalence-class",
      "title": "Untitled 10",
      "relations": [
        {
          "type": "relation",
          "relationType": "support",
          "from": "Untitled 11",
          "fromType": "equivalence-class",
          "to": "Untitled 10",
          "toType": "equivalence-class"
        },
        {
          "type": "relation",
          "relationType": "support",
          "from": "Untitled 10",
          "fromType": "equivalence-class",
          "to": "Untitled 9",
          "toType": "equivalence-class"
        }
      ],
      "members": [
        {
          "type": "statement",
          "role": "relation-statement",
          "text": "We are in danger of developing a relationship with AI that leaves us much less connected to each other.",
          "title": "Untitled 10",
          "startLine": 11,
          "startColumn": 5,
          "endLine": 12,
          "endColumn": 89
        }
      ],
      "isUsedAsRelationStatement": true,
      "fontColor": "#000000",
      "color": "#1b9e77"
    },
    "Untitled 11": {
      "type": "equivalence-class",
      "title": "Untitled 11",
      "relations": [
        {
          "type": "relation",
          "relationType": "support",
          "from": "Untitled 11",
          "fromType": "equivalence-class",
          "to": "Untitled 10",
          "toType": "equivalence-class"
        }
      ],
      "members": [
        {
          "type": "statement",
          "role": "relation-statement",
          "text": "The temptation to outsource our creative work to AI is strong and growing stronger",
          "startLine": 12,
          "startColumn": 7,
          "endLine": 12,
          "endColumn": 89,
          "title": "Untitled 11"
        }
      ],
      "isUsedAsRelationStatement": true,
      "fontColor": "#000000",
      "color": "#1b9e77"
    },

      "members": [
        {
          "type": "statement",
          "role": "top-level-statement",
          "isTopLevel": true,
          "text": "The paper’s main lesson is that an effective theory of analogy integrates both semantic (concept/conceptualist) structure and contextual/judgmental analysis.",
          "title": "Untitled 1",
          "startLine": 1,
          "startColumn": 1,
          "endLine": 17,
          "endColumn": 26
        }
      ],
      "isUsedAsTopLevelStatement": true,
      "fontColor": "#000000",
      "color": "#1b9e77"
    },
    "Untitled 2": {
      "type": "equivalence-class",
      "title": "Untitled 2",
      "relations": [
        {
          "type": "relation",
          "relationType": "support",
          "from": "Untitled 3",
          "fromType": "equivalence-class",
          "to": "Untitled 2",

      "members": [
        {
          "type": "statement",
          "role": "top-level-statement",
          "isTopLevel": true,
          "text": "This is a rich edition.",
          "title": "Untitled 15",
          "startLine": 25,
          "startColumn": 1,
          "endLine": 26,
          "endColumn": 172,
          "section": "s1"
        }

      "members": [
        {
          "type": "statement",
          "role": "relation-statement",
          "text": "Gregory Currie rejects weak illusionism, arguing that cinematic images really move, but in a response-dependent way.",
          "title": "Untitled 2",
          "startLine": 4,
          "startColumn": 7,
          "endLine": 10,
          "endColumn": 160,
          "section": "s1"
        }
      ],
      "section": "s1",
      "isUsedAsRelationStatement": true,

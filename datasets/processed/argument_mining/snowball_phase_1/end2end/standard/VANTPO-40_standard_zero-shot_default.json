    "ADU23": {
      "type": "Major Claim",
      "text": "\"Bayle's failed attempt to save his career at the Illustrious School of Rotterdam by turning to Erasmus tapped into a long",
      "isImplicit": false
    },
    "ADU24": {
      "type": "Claim",
      "text": "\"Rotterdam has always been associated with trade and commerce",
      "isImplicit": false
    },
    "ADU25": {
      "type": "Claim",
      "text": "\"The appropriation of Erasmus has also produced a wide variety of politically motivated initiatives",
      "isImplicit": false
    },
    "ADU26": {
